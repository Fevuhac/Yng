<div class="col-md-6">
  <section class="panel">
    <header class="panel-heading">
      <div class="panel-actions">
        <a href="#" class="panel-action panel-action-toggle" data-panel-toggle=""></a>
        <a href="#" class="panel-action panel-action-dismiss" data-panel-dismiss=""></a>
      </div>

      <h2 class="panel-title">
        <%= txt_title_control_pool %>
        </h2>
      <p class="panel-subtitle">
        <%= txt_content_control_pool %>
        </p>
    </header>
    <div class="panel-body">

      <!-- content start -->

      <div>
        <p class="refresh_jackpot"><button id="btn_refresh_jackpot"><%= txt_refresh %></button><%= txt_left_time %><b>60</b><%= txt_unit_second %></p>
      </div>

      <table border="2" bordercolor="#000000" style="width: 100%;" id="query-table-orders">
        <tr>
            <th class="cell-padding cell-align-center cell-head"
                style="border: solid thin #000000;background-color: #dfdfdf;text-align:center;">
                <%= txt_key %>
            </th>
            <th class="cell-padding cell-align-center cell-head"
                style="border: solid thin #000000;background-color: #dfdfdf;text-align:center;">
                <%= txt_val %>
            </th>
        </tr>
        <tr id="fake-data-order-list">
            <td style="border: solid thin #000000" class="cell-padding cell-align-center"><%= txt_total_recharge %></td>
            <td style="border: solid thin #000000" class="cell-padding cell-align-center" id="recharge"><b></b></td>
        </tr>
        <tr id="fake-data-order-list">
            <td style="border: solid thin #000000" class="cell-padding cell-align-center"><%= txt_total_cash %></td>
            <td style="border: solid thin #000000" class="cell-padding cell-align-center" id="cash"><b></b></td>
        </tr>
        <tr id="fake-data-order-list">
            <td style="border: solid thin #000000" class="cell-padding cell-align-center"><%= txt_total_gold %></td>
            <td style="border: solid thin #000000" class="cell-padding cell-align-center" id="totalGold"><b></b></td>
        </tr>
        <tr id="fake-data-order-list">
            <td style="border: solid thin #000000" class="cell-padding cell-align-center"><%= txt_give_gold %></td>
            <td style="border: solid thin #000000" class="cell-padding cell-align-center" id="give"><b></b></td>
        </tr>
        <tr id="fake-data-order-list">
            <td style="border: solid thin #000000" class="cell-padding cell-align-center"><%= txt_bonus_pool %></td>
            <td style="border: solid thin #000000" class="cell-padding cell-align-center" id="bonusPool"><b></b></td>
        </tr>
        <tr id="fake-data-order-list">
            <td style="border: solid thin #000000" class="cell-padding cell-align-center"><%= txt_pump_pool %></td>
            <td style="border: solid thin #000000" class="cell-padding cell-align-center" id="pump"><b></b></td>
        </tr>
        <tr id="fake-data-order-list">
            <td style="border: solid thin #000000" class="cell-padding cell-align-center"><%= txt_warning_coefficiancy %></td>
            <td style="border: solid thin #000000" class="cell-padding cell-align-center" id="warning"><b></b></td>
        </tr>
      </table>
      <div><%= txt_server_catch_rate %></div>
        <div id="containerSliderJackpot" class="mt-lg mb-lg slider-primary"
            data-plugin-slider data-plugin-options='{ "value": 100, "range": false, "min":50, "max": 150 }'
            data-plugin-slider-output="#listenSliderJackpot">
            <input id="listenSliderJackpot" type="hidden" value="100" />
        </div>
        <p class="output_jackpot"><%= txt_current_value %> <b>100</b>%</p>

        <div>
            <label><%= txt_expire_time %></label>
            <input id="input-timeout" name="txt" type="number" class="form-control input-lg" value="60"/>
        </div>

        <button id="btn_change_rate_jackpot"><%= txt_commit %></button>

      <!-- content end -->
      
    </div>
  </section>
</div>